public class Cubo extends FiguraGeometrica3D {

	private float arista;
	
	
	public Cubo(float arista)
	{
		this.arista = arista;
	}
	
	public flaot setArista() 
	{
		return arista;
	}
	
	
	@Override
	public float calcularAreaFigura() {
		
		return 0;
	}

}
