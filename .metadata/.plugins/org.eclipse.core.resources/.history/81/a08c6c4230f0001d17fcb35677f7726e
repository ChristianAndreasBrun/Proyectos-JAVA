package brun.christian.damw.m03.uf02.t03.colecciones;

public class Coche {

	private int num;
	private String marca;
	private String modelo;

	public Coche() {
    }

	public Coche(int num, String marca, String modelo) {
        this.num = num;
        this.marca = marca;
        this.modelo = modelo;
    }

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	public String getNombre() {
		return marca;
	}

	public void setNombre(String marca) {
		this.marca = marca;
	}

	public int getEdad() {
		return modelo;
	}

	public void setEdad(int modelo) {
		this.modelo = modelo;
	}
}
